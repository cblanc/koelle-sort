{
  "name": "@cablanchard/koelle-sort",
  "version": "0.1.0",
  "description": "koelle-sort - also known as alphanum sort",
  "repository": {
    "type": "git",
    "url": "git@github.com:cblanc/koelle-sort"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npm run lint && NODE_ENV=test node_modules/.bin/nyc node_modules/.bin/mocha",
    "lint": "node_modules/.bin/tslint -c tslint.json -p tsconfig.json lib/**/*.ts",
    "build": "npm run lint && node_modules/.bin/tsc",
    "benchmark": "node_modules/.bin/ts-node benchmark.js",
    "coverage": "node_modules/.bin/nyc report --reporter=text-lcov | node_modules/.bin/coveralls || true",
    "prepare": "npm run build"
  },
  "mocha": {
    "spec": "tests/*.ts",
    "exit": true,
    "fullTrace": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "nyc": {
    "include": [
      "lib/**/*.ts"
    ],
    "exclude": [
      "dist/*"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  },
  "engines": {
    "node": ">=6.9"
  },
  "author": "cblanc",
  "license": "MIT",
  "devDependencies": {
    "@types/benchmark": "~1.0.31",
    "@types/chai": "~4.2.0",
    "@types/mocha": "~7.0.1",
    "@types/node": "~13.9.0",
    "alphanumeric-sort": "0.0.1",
    "benchmark": "~2.1.4",
    "chai": "~4.2.0",
    "coveralls": "~3.0.2",
    "javascript-natural-sort": "~0.7.1",
    "mocha": "~7.1.0",
    "natural-orderby": "~2.0.0",
    "natural-sort": "~1.0.0",
    "nyc": "~15.0.0",
    "source-map-support": "~0.5.6",
    "ts-node": "~8.7.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.2",
    "typings": "~2.1.1"
  },
  "dependencies": {}
}
